<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
    http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="cricketCoach" class="com.practice.springdemo.CricketCoach">
	</bean>
	<bean id="footBallCoach"
		class="com.practice.springdemo.FootballCoach">
	</bean>

	<bean id="myLoggerConfig"
		class="com.lov2code.springdemo.MyLoggerConfig"
		init-method="initLogger">
		<property name="rootLoggerLevel" value="FINEST" />
		<property name="printedLoggerLevel" value="FINEST" />
	</bean>
</beans>